{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft": "Warning",
      "Microsoft.Hosting.Lifetime": "Information"
    }
  },
  "AllowedHosts": "*",
  "ApiConsistency": {
    "IsDebug": false,
    "ShowStatusCode": true,
    "ShowApiVersion": false,
    "ApiVersion": "1.0",
    "IgnoreNullValue": true,
    "UseCamelCaseNaming": true,
    "EnableExceptionLogging": true
  }
}